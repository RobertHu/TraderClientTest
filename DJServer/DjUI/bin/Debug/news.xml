<codes codetype="subject">L/ENGST,L/STORY,L/EN,J/CFF,J/COS,J/CFW,N/CLSNEW,N/AER,N/BKG,N/CMDI,N/CMR,N/DJAE,N/DJG,N/DJG7,N/DJGP,N/DJGS,N/DJGV,N/DJI,N/DJIV,N/DJMB,N/DJMO,N/DJMS,N/DJSN,N/DN,N/EWR,N/FXW,N/WED,N/WER,N/APIN,N/BON,N/CBK,N/CCS,N/DFF,N/DJWI,N/DRV,N/EMP,N/FXTR,N/MKC,N/MKT,N/NJR,N/SDT,N/TSY,PAG/1037,PAG/4153,M/JPY,M/NND,M/TPX,G/IGV,G/JABJ,G/JAGV,G/JAMF,P/AEQ,P/PS,T/CFW,N/CFW,T/COS,N/CONS,T/CFF,N/CFF</codes>
